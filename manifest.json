{
    "manifest_version": 3,
    "name": "osinter",
    "description": "Customizable OSINT search tool",
    "version": "0.0.1",
    "background": {
        "service_worker": "script.js"
    },
    "permissions": [
        "contextMenus",
        "storage",
        "tabs"
    ],
    "options_page": "settings/settings.html"
}