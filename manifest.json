{
    "manifest_version": 3,
    "name": "Osinter",
    "description": "Customizable OSINT tool for quick artifact lookup",
    "version": "1.0.1",
    "background": {
        "service_worker": "script.js"
    },
    "permissions": [
        "contextMenus",
        "storage",
        "tabs"
    ],
    "options_page": "settings/settings.html"
}