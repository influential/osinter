{
    "manifest_version": 3,
    "name": "Osinter",
    "description": "Customizable OSINT tool for quick artifact lookup",
    "version": "1.0.3",
    "icons": {
        "16": "images/16x16.png",
        "32": "images/32x32.png",
        "48": "images/48x48.png",
        "128": "images/128x128.png"
    },
    "background": {
        "service_worker": "script.js"
    },
    "permissions": [
        "contextMenus",
        "storage"
    ],
    "options_page": "settings/settings.html"
}